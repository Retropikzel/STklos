#
# Editoconfig for STklos development tree
#

root = true

[*]
indent_style             = space
indent_size              = 4
end_of_line              = lf
charset                  = utf-8
trim_trailing_whitespace = true

[*.{c,h}]
indent_size = 2

[Makefile{,.am}]
indent_style = tab
indent_size  = 8



