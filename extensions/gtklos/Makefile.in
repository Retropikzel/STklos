.PHONY: demo

SUBDIRS= src demos

all:
	for f in $(SUBDIRS) ;do (cd $$f; $(MAKE) $@) ;done

clean:
	for f in $(SUBDIRS) ;do (cd $$f; $(MAKE) $@) ;done
	rm -f *~


demo:
	(cd demos; make demo)


install:


distclean: clean
	for f in $(SUBDIRS) ;do (cd $$f; $(MAKE) $@) ;done
	rm -f Makefile

