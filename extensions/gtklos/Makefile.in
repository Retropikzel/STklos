.PHONY: all clean demo install distclean

all:
	(cd src; $(MAKE) $@)
	(cd demos; $(MAKE) $@)

demo:
	(cd demos; $(MAKE) $@)


install:


clean:
	rm -f *~
	(cd src; $(MAKE) $@)
	(cd demos; $(MAKE) $@)


distclean: clean
	(cd src; $(MAKE) $@)
	(cd demos; $(MAKE) $@)
	rm -f Makefile

