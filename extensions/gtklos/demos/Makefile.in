EXE=$(SRC:.stk=)

%: %.stk demos-utils.stk
	stklos-compile -c -o $* $<

all: 	$(EXE)
	@echo $(EXE) > all-demos

clean:
	/bin/rm -f $(EXE) *~ all-demos

distclean:
	/bin/rm -f stklos-gtklos.so Makefile


